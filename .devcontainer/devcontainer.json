{
    "name": "Github Codespaces Container",
    "postCreateCommand": "yarn install",
    "postAttachCommand": {
        "server": "npx supabase start",
        "waitFor": "postCreateCommand"
    },
    "forwardPorts": [54321, 54323],
    "portsAttributes": {
        "54321": {
            "label": "Supabase Studio",
            "onAutoForward": "notify"
        },
        "54323": {
            "label": "Edge Functions",
            "onAutoForward": "notify"
        }
    }
}
